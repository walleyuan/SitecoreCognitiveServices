@model Microsoft.ProjectOxford.Vision.Contract.Tag[]

@if (Model != null)
{
    <div class="detail-box detail-tags">
        <h3>Tags</h3>
        <div class="detail-row">
            @foreach (var t in Model)
            {
                <div class="tag-set">
                    <label>@t.Name: </label>
                    @Html.Partial("~/Views/CognitiveAnalysis/Common/MeasureBar.cshtml", (float)t.Confidence)
                </div>
            }
        </div>
    </div>
}